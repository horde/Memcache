{
    "minimum-stability": "dev",
    "name": "horde/memcache",
    "description": "Memcache client library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Memcache",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2021-11-05",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "php": "^7 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "ext-hash": "*"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "horde/log": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "ext-memcache": "^2",
        "ext-memcached": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Memcache": "lib/"
        },
        "psr-4": {
            "Horde\\Memcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Memcache\\Test\\": "test/"
        }
    }
}